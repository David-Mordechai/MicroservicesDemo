{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": { "expression": "@mt = 'An unhandled exception has occurred while executing the request.'" }
      }
    ],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://aero_seq:5341" }
      }
    ],
    "Properties":{
      "ApplicationName": "NotificationService"
    }
  },

  "Settings": {
    "BrokerService": "aero_rabbitmq",
    "NewMapTopic": "NewMap",
    "NewMapClientMethod": "NewMap",
    "NewMapPointTopic": "NewMapPoint",
    "NewMapPointClientMethod": "NewMapPoint"
  },
  "AllowedHosts": "*"
}
